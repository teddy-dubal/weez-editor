{
    "name": "vendor/editor-pdf",
    "description": "Description of project EditorPdf.",
    "authors": [
        {
            "name": "T.ED",
            "email": "t.erembertdubal@weezevent.com"
        }
    ],
    "require": {
        "php": ">=5.5",
        "spipu/html2pdf": "v4.6.1",
        "imagine/imagine": "v0.6.3",
        "enyo/dropzone": "v4.3.0",
        "kangax/fabric.js": "dev-master",
        "components/jquery": "~2",
        "components/underscore": "1.8.3",
        "components/bootstrap": "3.3.6",
        "uxder/Radio": "dev-master",
        "twig/twig": "dev-master",
        "paquettg/php-html-parser": "^1.7"
    },
    "repositories": [
        {
            "type": "package",
            "package": {
                "name": "kangax/fabric.js",
                "type": "component",
                "version": "master",
                "dist": {
                    "url": "https://github.com/kangax/fabric.js/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/kangax/fabric.js.git",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "dist/*"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "uxder/Radio",
                "type": "component",
                "version": "master",
                "dist": {
                    "url": "https://github.com/uxder/Radio/archive/master.zip",
                    "type": "zip"
                },
                "source": {
                    "url": "https://github.com/uxder/Radio.git",
                    "type": "git",
                    "reference": "master"
                },
                "extra": {
                    "component": {
                        "scripts": [
                            "radio.min.js"
                        ]
                    }
                },
                "require": {
                    "robloach/component-installer": "*"
                }
            }
        }
    ]
}
